datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Item {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
}

model Example {
  id             Int      @id @default(autoincrement())
  persona        String
  model          String
  tree           Json
  rootQuestion   String
  normalizedRoot String   @unique
  createdAt      DateTime @default(now())
}
